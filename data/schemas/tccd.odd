               <elementSpec ident="person" module="namesdates" mode="change">
                  <constraintSpec scheme="schematron" ident="personIdNoClashWithPlace" mode="add">
                     <desc>The <att>xml:id</att> attributes of person and place elements should not clash.</desc>
                     <constraint>
                        <sch:assert test="not(doc-available('../placeography/placeography.xml')) or not(@xml:id = doc('../placeography/placeography.xml')//tei:place/@xml:id)">
                           The xml:ids of person elements must not clash with those of place elements
                           in the placeography file.
                        </sch:assert>
                     </constraint>
                  </constraintSpec>
               </elementSpec>
               <elementSpec ident="place" module="namesdates" mode="change">
