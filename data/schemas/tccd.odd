               <elementSpec ident="TEI" module="textstructure" mode="change">
                  <constraintSpec ident="rootIdEqualsFileName" scheme="schematron">
                     <desc>The root TEI element must have an @xml:id attribute which 
              matches its filename.</desc>
                     <constraint>
                        <sch:rule context="tei:TEI">
                           <sch:let name="reqId"
                                    value="substring-before(tokenize(document-uri(/), '/')[last()], '.xml')"/>
                           <sch:let name="unexpandedId" value="substring-before($reqId, '_expanded')"/>
                           <sch:assert test="@xml:id = ($reqId, $unexpandedId)"> 
                    The @xml:id attribute on the TEI element (<sch:value-of select="@xml:id"/>) should match the 
                    document filename without extension (<sch:value-of select="$reqId"/>).
                  </sch:assert>
                           <sch:report role="warning" test="$reqId = concat($unexpandedId, '_expanded')">
                              Check this file to make sure it has been expanded correctly.
                              If it has, re-save it over the original template file, and delete
                              the file with "_expanded" in its filename.
                           </sch:report>
                        </sch:rule>
                     </constraint>
                  </constraintSpec>
                  <constraintSpec ident="curlyQuotesNotAllowed" scheme="schematron">
                     <desc>This project has standardized on "straight" quotes, single and 
                     double. Curly quotes are not allowed.</desc>
                     <constraint>
                        <sch:rule context="tei:TEI">
                           <sch:assert test="not(matches(., '[“”‘’]'))"> 
                              The file <sch:value-of select="@xml:id"/>.xml appears to use at least one
                              "curly" quote or apostrophe (“”‘’). Please use the straight versions of 
                              quotation marks and apostrophes throughout. 
                           </sch:assert>
                        </sch:rule>
                     </constraint>
                  </constraintSpec>
                  <constraintSpec ident="shouldHaveXmlLang" scheme="schematron">
                     <desc>All debate day document should have an xml:lang attribute on the
                     root element.</desc>
                     <constraint>
                        <sch:rule context="tei:TEI">
                           <sch:assert test="@xml:lang or not(starts-with(@xml:id, 'lg'))">
                              All debate day documents should have an @xml:lang attribute on the root
                              element; French documents should be xml:lang="fr" and English documents
                              should be xml:lang="en".
                           </sch:assert>
                        </sch:rule>
                     </constraint>
                  </constraintSpec>
                  <!-- We'll enforce the constraint that curly quotes are not allowed using Schematron
                    at the TEI level. -->
               </elementSpec>
               <!--          We have a very limited structure for text body elements: divs and 
              milestones are allowed. -->
               <elementSpec ident="body" module="textstructure" mode="change">
                  <content>
                     <alternate minOccurs="1" maxOccurs="unbounded">
                        <elementRef key="fw"/>
                        <elementRef key="pb"/>
                        <elementRef key="cb"/>
                        <elementRef key="milestone"/>
                        <elementRef key="div"/>
