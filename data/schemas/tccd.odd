                     <desc>The <att>xml:id</att> attributes of place and person elements should not clash.</desc>
                     <constraint>
                        <sch:assert test="not(doc-available('../personography/personography.xml')) or not(@xml:id = doc('../personography/personography.xml')//tei:person/@xml:id)">
