                        <sch:let name="apos.typographic" value="'‘’'"/>
                        <sch:let name="apos.straight" value="''''"/>
                        <sch:let name="quotes" value="concat($apos.straight, '&#34;')"/>
                        <sch:let name="punctuation" value="'\.\?!\(\):;,\[\]…'"/>
                     </sch:pattern>
                  </constraint>
               </constraintSpec>
