                     <desc>The <att>xml:id</att> attributes of person and place elements should not clash.</desc>
                     <constraint>
                        <sch:assert test="not(@xml:id = doc('../placeography/placeography.xml')//tei:place/@xml:id)">
                           The xml:ids of person elements must not clash with those of place elements
                           in the placeography file.
                        </sch:assert>
                     </constraint>
                  </constraintSpec>
               </elementSpec>
               <elementSpec ident="place" module="namesdates" mode="change">
                  <constraintSpec scheme="schematron" ident="placeIdNoClashWithPerson" mode="add">
                     <desc>The <att>xml:id</att> attributes of place and person elements should not clash.</desc>
                     <constraint>
                        <sch:assert test="not(@xml:id = doc('../personography/personography.xml')//tei:person/@xml:id)">
                           The xml:ids of place elements must not clash with those of person elements
                           in the personography file.
                        </sch:assert>
                     </constraint>
                  </constraintSpec>
                  <attList>
                     <attDef ident="type" mode="change">
